
<% filterStatusHelper = (linkPrefix, statusFilter, keyword) => {
    let xhtmlResult = ''; 
    statusFilter.forEach((item, index) =>{
        let link = linkPrefix + 'status/' + item.value;
        if(keyword !== "" && keyword !== undefined) link += '?keyword=' + keyword;

        xhtmlResult += `<a class="btn m-b-sm btn-${item.class} btn-sm" href="${link}">
        ${item.name.toUpperCase() + " (" + item.count +")"}
        </a> `;
     })
     return xhtmlResult;
} %>

